  issn-linking    = {1367-4803},
  issue           = {13},
  keywords        = {Algorithms; Base Composition; Base Sequence; Models, Statistical; Nucleic Acid Conformation; Nucleotides, analysis; RNA, chemistry; Sequence Analysis, RNA; Software; Thermodynamics},
  nlm             = {PMC3694657},
  nlm-id          = {9808944},
  owner           = {NLM},
  pii             = {btt217},
  pmc             = {PMC3694657},
  pmid            = {23812999},
  pubmodel        = {Print},
  pubstatus       = {ppublish},
