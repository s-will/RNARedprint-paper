@Article{Garcia-Martin2015,
  author          = {Garcia-Martin, Juan Antonio and Dotu, Ivan and Clote, Peter},
  title           = {{RNAiFold 2.0:} a web server and software to design custom and Rfam-based {RNA} molecules.},
